{
  "name": "next-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:start": "next build && next start",
    "stylelint": "npx stylelint \"./**/*.{css,scss,sass}\" --config \"./stylelint.config.js\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "eslint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "test:unit": "jest --watch",
    "test:e2e:open": "start-server-and-test build:start http-get://localhost:3000 cypress:open",
    "test:e2e:run": "start-server-and-test build:start http-get://localhost:3000 cypress:run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "clsx": "^1.1.1",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "concurrently": "^7.0.0",
    "cypress": "^9.5.1",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "msw": "^0.39.1",
    "postcss-extend": "^1.0.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.4.2",
    "postcss-reporter": "^7.0.5",
    "precss": "^4.0.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.5.3",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "4.6.2"
  }
}
